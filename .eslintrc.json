{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "jsx-a11y/label-has-for": [
      2,
      {
        "components": [
          "Label"
        ],
        "required": {
          "every": [
            "nesting",
            "id"
          ]
        },
        "allowChildren": false
      }
    ]
  }
}
